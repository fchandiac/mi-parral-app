{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mi-parral/src",
  "monorepo": true,
  "root": "apps/mi-parral",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mi-parral/tsconfig.app.json"
  },
  "projects": {
    "mi-parral": {
      "type": "application",
      "root": "apps/mi-parral",
      "entryFile": "main",
      "sourceRoot": "apps/mi-parral/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mi-parral/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "auth-app": {
      "type": "application",
      "root": "apps/auth-app",
      "entryFile": "main",
      "sourceRoot": "apps/auth-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-app/tsconfig.app.json"
      }
    },
    "images-app": {
      "type": "application",
      "root": "apps/images-app",
      "entryFile": "main",
      "sourceRoot": "apps/images-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/images-app/tsconfig.app.json"
      }
    },
    "coupons-app": {
      "type": "application",
      "root": "apps/coupons-app",
      "entryFile": "main",
      "sourceRoot": "apps/coupons-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/coupons-app/tsconfig.app.json"
      }
    }
  }
}